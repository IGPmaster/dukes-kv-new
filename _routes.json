{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/build/*",
    "/static/*",
    "/_nuxt/*"
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(blog|promotion)/.*",
      "status": 200,
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": ".*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
